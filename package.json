{
  "name": "@jphilipstevens/lru-cache",
  "version": "0.0.1",
  "private": true,
  "description": "A simple TS implementation of an LRU cache",
  "keywords": [],
  "author": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engine": {
    "node": "14.x"
  },
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run test",
    "build": "npm run compile",
    "clean": "rm -rf ./node_modules/.cache && rm -rf ./lib ./coverage",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:fix": "tslint --fix -t verbose -c ./tslint.json 'src/**/*.ts'",
    "test": "npm run test:spec",
    "test:spec": "jest --json --outputFile=result.json --testLocationInResults",
    "test:integration": "jest --config=./jest.integration.config.js",
    "commit": "npx git-cz",
    "semantic-release": "semantic-release",
    "semantic-release:test": "semantic-release --dry-run --ci false"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.9",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "condition-circle": "^2.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.2.1",
    "semantic-release": "^15.13.12",
    "ts-node": "^9.1.1",
    "typescript": "latest",
    "uuid": "^8.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
